. /etc/mx-security/mxsec_functions

install () {
	install_file "snmpd.conf" "level1"
	install_file "000-default.conf" "level1"
	install_file ".htpasswd" "level1"
}

case "$1" in
	install)
		install
		;;
	*)
		echo -n -e "The parameter is incorrect."
		exit 1
		;;
esac

exit 0
